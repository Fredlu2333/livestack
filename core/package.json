{
  "name": "@livestack/core",
  "version": "0.0.2",
  "description": "Orchestration, in real-time.",
  "scripts": {
    "build": "preconstruct build",
    "postinstall": "preconstruct dev"
  },
  "license": "MIT",
  "dependencies": {
    "@livestack/shared": "*",
    "@livestack/vault-client": "*",
    "@livestack/vault-interface": "*",
    "@ltd/j-toml": "^1.38.0",
    "ansis": "^2.3.0",
    "dotenv": "^16.x",
    "lodash": "^4.17.21",
    "lru-cache": "^10.2.0",
    "p-limit": "^3",
    "rxjs": "^7",
    "uuid": "^9",
    "winston": "^3.12.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.4"
  },
  "devDependencies": {
    "@preconstruct/cli": "^2.8.4",
    "@types/lodash": "^4.14.201",
    "@types/uuid": "^9.0.7",
    "typescript": "5.4.5"
  },
  "optionalDependencies": {
    "@livestack/vault-dev-server": "*"
  },
  "module": "dist/livestack-core.esm.js",
  "main": "dist/livestack-core.cjs.js",
  "types": "dist/livestack-core.cjs.d.ts",
  "exports": {
    ".": {
      "import": "./dist/livestack-core.esm.js",
      "require": "./dist/livestack-core.cjs.js"
    },
    "./utils": {
      "import": "./utils/dist/livestack-core-utils.esm.js",
      "require": "./utils/dist/livestack-core-utils.cjs.js"
    },
    "./storage": {
      "import": "./storage/dist/livestack-core-storage.esm.js",
      "require": "./storage/dist/livestack-core-storage.cjs.js"
    },
    "./liveflow": {
      "import": "./liveflow/dist/livestack-core-liveflow.esm.js",
      "require": "./liveflow/dist/livestack-core-liveflow.cjs.js"
    },
    "./logging": {
      "import": "./logging/dist/livestack-core-logging.esm.js",
      "require": "./logging/dist/livestack-core-logging.cjs.js"
    },
    "./onboarding": {
      "import": "./onboarding/dist/livestack-core-onboarding.esm.js",
      "require": "./onboarding/dist/livestack-core-onboarding.cjs.js"
    }
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "storage.ts",
      "liveflow.ts",
      "utils.ts",
      "logging.ts",
      "onboarding.ts"
    ]
  },
  "author": ""
}
